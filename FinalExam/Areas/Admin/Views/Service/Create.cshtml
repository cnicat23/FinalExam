@model Service

<div class="container">
    <div class="row">
        <div class="col-12">
            <form asp-action="Create" method="post" enctype="multipart/form-data">
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Title</label>
                    <input asp-for="Title" class="form-control" id="exampleInputEmail1">
                    <span asp-validation-for="Title"></span>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Description</label>
                    <input asp-for="Description" class="form-control" id="exampleInputEmail1">
                    <span asp-validation-for="Description"></span>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">SubDescription</label>
                    <input asp-for="SubDescription" class="form-control" id="exampleInputEmail1">
                    <span asp-validation-for="SubDescription"></span>
                </div>
                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Image</label>
                    <input asp-for="ImageFile" class="form-control" id="exampleInputEmail1">
                    <span asp-validation-for="ImageFile"></span>
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>
